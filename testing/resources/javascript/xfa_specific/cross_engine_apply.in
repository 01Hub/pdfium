          var result = app.setTimeOut.apply(xfa.host, 'var x = 3;', 10);
          app.alert('unexpectedly returned ' + result);
        }
        catch (e) {
          app.alert('Caught: ' + e);
        }
        app.alert('Applying xfa.host method to app');
        try {
          var result = xfa.host.setFocus.apply(app);
          app.alert('unexpectedly returned ' + result);
        }
        catch (e) {
          app.alert('Caught: ' + e);
        }
      </script>
    </event>
